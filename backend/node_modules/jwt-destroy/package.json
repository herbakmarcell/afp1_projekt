{
  "name": "jwt-destroy",
  "version": "3.5.7",
  "description": "JWT-Destroy is an enhanced version of the popular JSONWebToken npm package. It extends its functionality by offering manual expiry control, allowing developers to easily manage token expiration within their applications. With JWT-Destroy, you have more flexibility and control over token expiration, enhancing the security of your application.",
  "main": "./out/destroy.js",
  "types": "./out/destroy.d.ts",
  "scripts": {
    "test": "eslint source/**/*.ts --fix",
    "prepare": "npm test && npm run build",
    "build": "npm test && tsc",
    "lint": "eslint source/**/*.ts --fix"
  },
  "keywords": [
    "jwt",
    "jsonwebtoken",
    "jwt-destroy",
    "jwt-destroyer",
    "jwt-destroyed",
    "JWT-easily-destroyed"
  ],
